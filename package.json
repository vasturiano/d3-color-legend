{
  "name": "d3-color-legend",
  "version": "1.4.3",
  "description": "SVG color legends for continuous or discrete D3 scales",
  "type": "module",
  "jsdelivr": "dist/d3-color-legend.min.js",
  "unpkg": "dist/d3-color-legend.min.js",
  "main": "dist/d3-color-legend.mjs",
  "module": "dist/d3-color-legend.mjs",
  "exports": {
    "umd": "./dist/d3-color-legend.min.js",
    "default": "./dist/d3-color-legend.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vasturiano/d3-color-legend.git"
  },
  "keywords": [
    "D3",
    "legend",
    "color",
    "discrete",
    "continuous",
    "scales"
  ],
  "author": {
    "name": "Vasco Asturiano",
    "url": "https://github.com/vasturiano"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vasturiano/d3-color-legend/issues"
  },
  "homepage": "https://github.com/vasturiano/d3-color-legend#readme",
  "scripts": {
    "build": "rimraf dist && rollup -c",
    "dev": "rollup -w",
